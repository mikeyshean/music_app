<h1>Add a New Band</h1>

<p>
  <a href="<%= bands_url %>">Back to Bands</a>
</p>

<form action="<%= bands_url %>" method="post">
  <%= authenticity_token %>
  <label for="band_name"> Name: </label>
  <input
    type="text"
    name="band[name]"
    id="band_name"
    value="<%= @band.name %>">
  <br>

  <p><button>Submit</button></p>
</form>
